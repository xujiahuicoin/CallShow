# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

target 'CallShow' do
  # Comment the next line if you don't want to use dynamic frameworks
  use_frameworks!
#网络请求
pod 'Alamofire'
#自动布局
pod 'SnapKit'
#网络图片加载
pod 'Kingfisher'
#指示器
pod 'NVActivityIndicatorView'
#下拉刷新
pod 'ESPullToRefresh', '~> 2.9'

pod 'SwifterSwift'
#推送
pod 'JPush'
#HUD
pod 'PKHUD', '~> 5.2.1'
#json转Model
pod 'HandyJSON'
#键盘管理
pod 'IQKeyboardManagerSwift'
#动画库
pod 'ViewAnimator'
# 选项卡
pod 'Segmentio'
#Alert
pod 'SCLAlertView'

#google 广告
pod 'Google-Mobile-Ads-SDK'

end
